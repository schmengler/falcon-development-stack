{
  "debug": true,
  "port": 4000,
  "verboseEvents": false, 
  "apis": [
    {
      "name": "api-wordpress",
      "package": "@deity/falcon-wordpress-api",
      "config": {
        "host": "wordpress",
        "protocol": "http",
        "apiPrefix": "/wp-json",
        "apiSuffix": "/wp/v2"
      }
    },
    {
      "name": "api-magento2",
      "package": "@deity/falcon-magento2-api",
      "config": {
        "host": "magento2",
        "protocol": "http",
        "username": "node-api",
        "password": "3de3f3a262",

        "currency": {
          "symbol": "EUR"
        },

        "demo": true,
        "enableSwitcher": true,
        "enableAutoDetection": false,
        "geoMapping": {
          "*": "default",
          "NL": "nl_store",
          "PL": "pl_store",
          "FR": "fr_store",
          "DE": "de_store"
        },
        "languageMapping": {
          "en": ["*"],
          "nl": ["nl_store"],
          "pl": ["pl_store"],
          "de": ["de_store"],
          "fr": ["fr_store"]
        },
        "codeMapping": {
          "default": "default",
          "nl_store": "nl_store",
          "pl_store": "pl_store",
          "fr_store": "fr_store",
          "de_store": "de_store"
        }
      }
    }
  ],
  "extensions": [
    {
      "package": "@deity/falcon-blog-extension",
      "config": {
        "api": "api-wordpress"
      }
    },
    {
      "package": "@deity/falcon-shop-extension",
      "config": {
        "api": "api-magento2"
      }
    }
    }
  ],
  "session": {
    "keys": ["secret key"],
    "options": {
      "key": "session-key",
      "maxAge": 86400000,
      "overwrite": true,
      "httpOnly": true,
      "signed": true,
      "rolling": false,
      "renew": false
    }
  }
}
